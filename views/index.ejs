<!DOCTYPE html>
<html lang="en">
    <%-include('./partials/head.ejs')%>
<body>
    <%-include('./partials/nav.ejs')%> 
    <div class="blogs content">
        <h2>All resumes</h2>
        <% if(resumes.length > 0){ %>
            <% resumes.forEach(resume=>{%>
                <a class="single" href="/resumes/<%=resume.id%> ">
                    <h3 class="name"><%=resume.name%> Resume</h3>
                    <h4 class="name"><%=resume.email%></h4>
                </a>
                <br>
            <%})%>
        <% } %>
    </div>
</body>
<!-- <%-include('./partials/footer.ejs')%> -->
</html>
