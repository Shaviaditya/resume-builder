<!DOCTYPE html>
<html lang="en">
    <%-include('./partials/head.ejs')%>
<body>
    <%-include('./partials/nav.ejs')%> 
    <div class="create-blog content">
        <form action="/resumes" method="POST">
            <label for="name">Name</label>
            <input type="text" id="name" required name="name">
            <label for="email">Email:</label>
            <input type="email" id="email" required name="email">
            <label for="phone">Phone Number:</label>
            <input type="tel" id="phone" required name="phone">
            <label for="address">Address:</label>
            <input type="text" id="address" required name="address">
            <label for="education">Education:</label>
            <textarea id="education" required name="education"></textarea>
            <label for="experience">Experience:</label>
            <textarea id="experience" required name="experience"></textarea>
            <label for="skills">Skills:</label>
            <textarea id="skills" required name="skills"></textarea>
            <button>Submit</button>
        </form>
    </div>
</body>
<%-include('./partials/footer.ejs')%>
</html>
